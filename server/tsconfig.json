{
"compilerOptions": {
  "target": "ES2020",
  "module": "ESNext",
  "moduleResolution": "node",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "resolveJsonModule": true,
  "strict": true,
  "skipLibCheck": true,
  "outDir": "dist",
  "types": ["node"], 
  "forceConsistentCasingInFileNames": true
},
"include": ["src", "utils"]

}
